{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
      "jsx": "react",
      "module": "esnext",
      "target": "es6",
      "lib": ["dom", "es2015", "es2016", "es2017"],
      "allowJs": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "noEmit": true
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
  }